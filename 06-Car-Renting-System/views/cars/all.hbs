<h1>Cars</h1>

<div class="cars-container">
    {{#each cars}}
    <div class="car-container">
        <section>
            <a href="/cars/edit/{{_id}}">
                <img src="{{imageUrl}}" alt='car-image' />
            </a>
        </section>
        <section class="info">
            <strong>{{model}}</strong>
            <span class="rented">rented:
                <strong>1</strong>
            </span>
        </section>
    </div>
    {{/each}}
</div>